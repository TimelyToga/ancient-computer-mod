{
	"schemaVersion": 1,
	"id": "ancientcomputer",
	"version": "${version}",
	"name": "Ancient Computer",
	"description": "Adds Ancient Computers that can wirelessly transmit redstone signals across any distance.",
	"authors": [
		"Histograms4You"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/ancientcomputer/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.timbo.ancientcomputer.AncientComputerMod"
		],
		"client": [
			"com.timbo.ancientcomputer.AncientComputerClient"
		],
		"fabric-datagen": [
			"com.timbo.ancientcomputer.AncientComputerDataGenerator"
		]
	},
	"mixins": [
		"ancientcomputer.mixins.json",
		{
			"config": "ancientcomputer.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}