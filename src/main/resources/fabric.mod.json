{
	"schemaVersion": 1,
	"id": "tutorialmod",
	"version": "${version}",
	"name": "TutorialMod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Histograms4You"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/tutorialmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.timbo.tutorialmod.TutorialMod"
		],
		"client": [
			"com.timbo.tutorialmod.TutorialModClient"
		],
		"fabric-datagen": [
			"com.timbo.tutorialmod.TutorialModDataGenerator"
		]
	},
	"mixins": [
		"tutorialmod.mixins.json",
		{
			"config": "tutorialmod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}